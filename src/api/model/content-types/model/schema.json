{
  "kind": "collectionType",
  "collectionName": "models",
  "info": {
    "singularName": "model",
    "pluralName": "models",
    "displayName": "Model"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "title": {
      "type": "string"
    },
    "reports": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::report.report",
      "mappedBy": "model"
    }
  }
}
